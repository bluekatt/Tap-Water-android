<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/navigation_record">

    <fragment
        android:id="@+id/navigation_summary"
        android:name="com.parkjongseok.tapwater.ui.summary.SummaryFragment"
        android:label="@string/title_summary"
        tools:layout="@layout/fragment_summary" />

    <fragment
        android:id="@+id/navigation_record"
        android:name="com.parkjongseok.tapwater.ui.record.RecordFragment"
        android:label="@string/title_record"
        tools:layout="@layout/fragment_record" />
    <fragment
        android:id="@+id/navigation_settings_host"
        android:name="com.parkjongseok.tapwater.ui.settings.SettingsNavHostFragment"
        android:label="fragment_settings_nav_host"
        tools:layout="@layout/fragment_settings_nav_host" />

</navigation>